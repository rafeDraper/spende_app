.navigation_bar data-controller="navbar"
  #mylinks 
    - if user_signed_in?
      p 
        | Angemeldet als:   
        strong = current_user.email
      = link_to fa_icon('home', text: "Home"), root_path
      = link_to fa_icon('id-card', text: "Profil"), edit_user_registration_path
      = link_to fa_icon('users-cog', text: "Nutzer"), users_path
      = link_to fa_icon('list-ul', text: "Listen"), meetings_lists_path
      = link_to fa_icon('sign-out-alt', text: "Abmelden"), destroy_user_session_path
    - else
      = link_to "Registrieren", new_user_registration_path
      = link_to "Anmelden", new_user_session_path
      = link_to "Hilfe", static_pages_help_path
      = link_to "Kontakt", static_pages_contact_path
  a href="javascript:void(0);" class="icon" data-action="click->navbar#nav_extend"
    i.fa.fa-bars